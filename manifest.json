{
    "manifest_version":3,
    "version":"1.0",
    "name":"OktaInfo, a browser extension to fix Okta's UI.",
    "content_scripts":[
      {
       "matches":["https://*.okta.com/admin/user/profile/view/*",
       "https://*.okta.com/api/v1/users/*"],
       "js":["content.js"],
       "css": ["css/style.css"]
      }
    ],
    "icons": {
      "16": "icon/icon.png"
    }
  }